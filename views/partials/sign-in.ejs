<form action="/addProfile" method="POST" enctype="multipart/form-data">
    <div class="mb-3">
        <label for="username" class="form-label">username</label>
        <input type="text" class="form-control" id="image" name="username" required>
    </div>
    <div class="mb-3">
        <label for="password" class="form-label">Password</label>
        <input type="password" class="form-control" id="storename" name="password" required>
    </div>
    <div class="mb-3">
        <label for="name" class="form-label">Owner Name</label>
        <input type="text" class="form-control" id="name" name="name" required>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>